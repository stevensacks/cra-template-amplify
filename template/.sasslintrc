{
    "files": {
        "include": "src/**/*.s+(a|c)ss"
    },
    "options": {
        "formatter": "stylish",
        "merge-default-rules": false
    },
    "rules": {
        "clean-import-paths": 0,
        "indentation": [
            1,
            {
                "size": 4
            }
        ],
        "leading-zero": [
            1,
            {
                "include": true
            }
        ],
        "nesting-depth": [
            1,
            {
                "max-depth": 8
            }
        ],
        "no-color-literals": 0,
        "no-misspelled-properties": [
            1
        ],
        "no-url-protocols": 0,
        "property-sort-order": [
            1,
            {
                "ignore-custom-properties": true,
                "order": [
                    "position",
                    "top",
                    "right",
                    "bottom",
                    "left",
                    "z-index",
                    "display",
                    "visibility",
                    "grid",
                    "grid-before",
                    "grid-after",
                    "grid-area",
                    "grid-auto-columns",
                    "grid-auto-flow",
                    "grid-auto-rows",
                    "grid-column",
                    "grid-column-start",
                    "grid-column-end",
                    "grid-column-gap",
                    "grid-columns",
                    "grid-start",
                    "grid-end",
                    "grid-gap",
                    "grid-row",
                    "grid-row-start",
                    "grid-row-end",
                    "grid-row-gap",
                    "grid-rows",
                    "grid-template",
                    "grid-template-areas",
                    "grid-template-columns",
                    "grid-template-rows",
                    "flex",
                    "flex-basis",
                    "flex-direction",
                    "flex-flow",
                    "flex-grow",
                    "flex-shrink",
                    "flex-wrap",
                    "justify-content",
                    "align-items",
                    "align-content",
                    "align-self",
                    "float",
                    "clear",
                    "overflow",
                    "overflow-x",
                    "overflow-y",
                    "clip",
                    "box-sizing",
                    "margin",
                    "margin-top",
                    "margin-right",
                    "margin-bottom",
                    "margin-left",
                    "padding",
                    "padding-top",
                    "padding-right",
                    "padding-bottom",
                    "padding-left",
                    "min-width",
                    "max-width",
                    "width",
                    "min-height",
                    "max-height",
                    "height",
                    "outline",
                    "outline-width",
                    "outline-style",
                    "outline-color",
                    "outline-offset",
                    "border",
                    "border-collapse",
                    "border-color",
                    "border-spacing",
                    "border-style",
                    "border-width",
                    "border-top",
                    "border-top-width",
                    "border-top-style",
                    "border-top-color",
                    "border-right",
                    "border-right-width",
                    "border-right-style",
                    "border-right-color",
                    "border-bottom",
                    "border-bottom-width",
                    "border-bottom-style",
                    "border-bottom-color",
                    "border-left",
